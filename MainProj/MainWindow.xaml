<Window x:Class="MainProj.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MainProj"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="350" Background="{x:Null}">
    <Image x:Name="myImage" HorizontalAlignment="Center" Height="350" Width="350" VerticalAlignment="Center" Grid.Column="0" Margin="0,0,0,0" MouseMove="myImage_MouseMove" MouseLeftButtonUp="myImage_MouseLeftButtonUp"/>
</Window>
