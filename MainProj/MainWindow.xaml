<Window x:Class="MainProj.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MainProj"
        mc:Ignorable="d"
        Title="MainWindow" Height="250" Width="250" Background="{x:Null}" Closed="Window_Closed">

    <Image x:Name="MainImage" HorizontalAlignment="Center" Height="250" Width="250" VerticalAlignment="Center" Grid.Column="0" Margin="0,0,0,0" MouseLeftButtonUp="MainImage_MouseLeftButtonUp" MouseMove="MainImage_MouseMove" />

</Window>
